<robot name="mybot">

	<!-- * * * Link Definitions * * * -->
	<link name="base">
		<visual>
			<origin xyz="0.25 0 0.35" rpy="1.5708 0 1.5708"/>
			<geometry>
				<box size="0.5 0.75 0.5"/>
			</geometry>
	     	<material name="red">
				<color rgba="0.9 0 0 1.0"/>
			</material>
	   </visual>
  </link>
 	<link name="right_rear_wheel">
		<visual>
			<origin xyz="0 0 0" rpy="1.5708 0 1.5708"/>
			<geometry>
				<cylinder radius="0.15" length="0.01"/>
			</geometry>
	     	<material name="green">
				<color rgba="0 0.9 0.9 1.0"/>
			</material>
	   </visual>
	</link>
	<link name="left_rear_wheel">
		<visual>
			<origin xyz="0 0 0" rpy="1.5708 0 1.5708"/>
			<geometry>
				<cylinder radius="0.15" length="0.01"/>
			</geometry>
	     	<material name="green">
				<color rgba="0 0.9 0.9 1.0"/>
			</material>
	   </visual>
	</link>
	<link name="front_wheel">
		<visual>
			<origin xyz="0 0 0" rpy="1.5708 0 1.5708"/>
			<geometry>
				<cylinder radius="0.15" length="0.01"/>
			</geometry>
	     	<material name="purple">
				<color rgba="0.9 0 0.9 1.0"/>
			</material>
	   </visual>
	</link>
	
 	<joint name="base_front" type="revolute">
    	<parent link="base"/>
    	<child link="front_wheel"/>
    	<origin xyz="0.5 0 0" rpy="0 0 0"/>
       	<limit upper="0.7" lower = "-0.7" effort="1" velocity="3"/>
        <axis xyz="0 0 1"/>
	</joint>
	<joint name="base_right_rear_wheel" type="revolute">
    	<parent link="base"/>
    	<child link="right_rear_wheel"/>
    	<origin xyz="0 0.3 0" rpy="0 0 0"/>
       	<limit upper="0.7" lower = "-0.7" effort="1" velocity="3"/>
        <axis xyz="0 0 1"/>
	</joint>
	<joint name="base_left_rear_wheel" type="revolute">
    	<parent link="base"/>
    	<child link="left_rear_wheel"/>
    	<origin xyz="0 -0.3 0" rpy="0 0 0"/>
       	<limit upper="0.7" lower = "-0.7" effort="1" velocity="3"/>
        <axis xyz="0 0 1"/>
	</joint>
 
</robot>